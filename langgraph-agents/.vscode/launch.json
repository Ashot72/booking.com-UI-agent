{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug LangGraph Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@langchain/langgraph-cli/dist/cli/cli.mjs",
      "args": ["dev"],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "stopOnEntry": false,
      "autoAttachChildProcesses": true
    },
    {
      "name": "Debug LangGraph Server (Attach)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
